<Window
    x:Class="EmploymentAgency.Views.Windows.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:EmploymentAgency.Views.Windows"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:mdt="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:vm="clr-namespace:EmploymentAgency.ViewModels"
    Title="Агентство трудоустройства"
    Width="820"
    Height="465"
    Background="{StaticResource WindowBackgroundBrush}"
    Content="{Binding PageSource}"
    DataContext="{Binding MainWindowViewModel, Source={StaticResource ViewModelLocator}}"
    Icon="/EmploymentAgency;component/Resources/Images/Icons/Icon.ico"
    mc:Ignorable="d" />
